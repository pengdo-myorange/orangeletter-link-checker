{
  "version": 2,
  "functions": {
    "api/fetch.py": {
      "runtime": "python3.9",
      "memory": 512,
      "maxDuration": 30
    },
    "api/scrape.py": {
      "runtime": "python3.9",
      "memory": 512,
      "maxDuration": 30
    }
  },
  "regions": ["icn1"],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}